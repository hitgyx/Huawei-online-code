#include<iostream>
#include<string>
using namespace std;

void inverse(string a){
    int num=a.length()-1;
    //cout<<num;
    string b;
    int i=0;
    for(;num>=0;num--){
        b+=a[num];
        i++;
    }
    cout<<b<<endl;

}
int main(){
    string a;
    while(getline(cin,a)){
        inverse(a);
    }
    return 0;
}

