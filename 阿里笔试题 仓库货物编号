#include<iostream>
#include<string>
#include<math.h>
using namespace std;

void Get(int n){
    int x;
    string a;

    char c[10];
    string str;
    //int num=1;
    str = a;
    for(int i=0;i<n;i++){
        int num=1;
        string b;
        for(int j=0;j<i+1;j++){
            itoa(num, c, 10);
            str=c;
            b+=str;
            num++;
        }
        a+=b;
    }
    cout<<a[110];


}
int main(){
    int n;
    cin>>n;
    Get(n);
    return 0;
}
